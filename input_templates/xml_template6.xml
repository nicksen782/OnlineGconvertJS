   <map var-name="slipiceF4" left="19" top="6" width="1" height="2" mapOutputTo="PROGMEM" />
   <map var-name="fireballF1" left="9" top="10" width="1" height="1" mapOutputTo="PROGMEM" />
   <map var-name="fireballF2" left="11" top="10" width="1" height="1" mapOutputTo="PROGMEM" />
   <map var-name="fireballF3" left="13" top="10" width="1" height="1" mapOutputTo="PROGMEM" />
   <map var-name="fireballF4" left="15" top="10" width="1" height="1" mapOutputTo="PROGMEM" />
  </maps>
 </output>
 </gfx-xform>
</TILESET>

<!--Bonus points-->

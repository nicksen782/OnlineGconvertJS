   <map var-name="shellscrapperF1" left="1" top="7" width="1" height="1" mapOutputTo="PROGMEM" />
   <map var-name="shellscrapperF2" left="3" top="7" width="1" height="1" mapOutputTo="PROGMEM" />
   <map var-name="shellscrapperF3" left="3" top="8" width="1" height="1" mapOutputTo="PROGMEM" />
   <map var-name="sidestepperF1" left="5" top="6" width="1" height="2" mapOutputTo="PROGMEM" />
   <map var-name="sidestepperF2" left="7" top="6" width="1" height="2" mapOutputTo="PROGMEM" />
   <map var-name="fighterflyF1" left="9" top="6" width="1" height="2" mapOutputTo="PROGMEM" />
   <map var-name="fighterflyF2" left="11" top="6" width="1" height="2" mapOutputTo="PROGMEM" />
   <map var-name="fighterflyF3" left="9" top="8" width="1" height="2" mapOutputTo="PROGMEM" />
   <map var-name="fighterflyF4" left="11" top="8" width="1" height="2" mapOutputTo="PROGMEM" />
   <map var-name="slipiceF1" left="13" top="6" width="1" height="2" mapOutputTo="PROGMEM" />
